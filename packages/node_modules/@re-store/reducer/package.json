{
    "name":"@re-store/reducer",
    "version": "1.0.0",
    "scripts": {
        "build": "rimraf ./dist && npm run build:esnext && npm run build:es6",
        "build:esnext": "tsc -p . --outDir ./dist/esm --module esnext --declaration --declarationDir ./dist/typings",
        "build:es6": "tsc -p . --outDir ./dist/cjs --module commonjs",
        "test": "cross-env TS_NODE_IGNORE=\"/(?<!packages\/)node_modules/\"  nyc ava"
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "typings": "./dist/typings/index.d.ts",
    "dependencies": {
        "@re-store/types": "*"
    }
}