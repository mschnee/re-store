{
  "name": "re-store",
  "version": "0.0.1",
  "description": "Opinionated redux state manager.",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "tslint -p .",
    "build": "npm run build:cjs && npm run build:esnext",
    "build:cjs": "tsc -p .",
    "build:esnext": "tsc -p . -d -m esnext --outDir ./dist/esnext --declarationDir ./dist/typings",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect && npm run build"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esnext/index.js",
  "author": "mschnee",
  "license": "MIT",
  "devDependencies": {
    "@types/expect": "^1.20.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "cross-env": "^5.2.0",
    "expect": "^24.0.0",
    "express": "^4.16.4",
    "jest": "^24.0.0",
    "jest-mock": "^24.0.0",
    "rimraf": "^2.6.3",
    "snyk": "^1.124.0",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.861",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "redux": "^4.0.1"
  }
}
