{
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
        "interface-name":false,
        "prettier": true,
        "semicolon": true,
        "indent": [true, "spaces", 4]
    }
}